{
  "name": "bienesraices-node-mvc",
  "version": "1.0.0",
  "description": "Bienes Raices",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "server": "npx nodemon src/index.js",
    "dev": "nodemon src/index.js | browser-sync start --files 'src/views/**/*.pug, public/**/*.*'",
    "css": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/styles.css --watch",
    "js": "npx webpack --watch",
    "client": "tailwindcss -i ./public/css/tailwind.css -o ./public/css/styles.css --watch | webpack --watch",
    "db:import": "node src/seed/seeder.js -i",
    "db:delete": "node src/seed/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielPAyala/bienesraices-node-mvc.git"
  },
  "keywords": [
    "MVC",
    "Pug",
    "Tailwind",
    "MySQL",
    "Sequelize"
  ],
  "author": "Carlos Daniel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielPAyala/bienesraices-node-mvc/issues"
  },
  "homepage": "https://github.com/DanielPAyala/bienesraices-node-mvc#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.7",
    "dropzone": "^5.9.3",
    "express": "^4.21.2",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.5",
    "nodemailer": "^6.9.16",
    "pug": "^3.0.3",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "browser-sync": "^3.0.3",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  }
}
